{
  "expo": {
    "name": "Twenty Timer",
    "description": "A minimalist 20-20-20 eye care timer",
    "slug": "twenty-timer",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4b250c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "githubUrl": "https://github.com/bdbaraban/twenty-timer"
  }
}
